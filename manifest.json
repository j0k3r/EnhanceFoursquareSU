{
    "name": "Enhance Foursquare SU",
    "version": "0.3",
    "description": "It makes some improvement to the Foursquare SU interface.",
    "homepage_url": "https://github.com/j0k3r/EnhanceFoursquareSU",
    "manifest_version": 2,
    "permissions": ["http://maps.googleapis.com/"],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "web_accessible_resources": [
        "js/vendor/jquery-2.0.3.min.js",
        "js/vendor/jquery-2.0.3.min.map",
        "js/enhance-su.js"
    ],
    "content_scripts": [{
        "matches": [
            "*://foursquare.com/edit/*",
            "*://*.foursquare.com/edit/*",
            "*://*.foursquare.com/v/*",
            "*://foursquare.com/v/*"
        ],
        "run_at": "document_end",
        "css": ["css/enhance-su.css"],
        "js": [
            "js/vendor/jquery-2.0.3.min.js",
            "js/contentscript.js"
        ]
    }]
}
